<view class="login"><view class="container"><view class="wrapper"><view class="left-top-sign">LOGIN</view><view class="welcome">注册</view><view class="input-content"><view class="input-item"><text class="tit">账号：</text><input type="text" name="username" placeholder="请输入您的账号:" data-event-opts="{{[['input',[['__set_model',['$0','username','$event',[]],['form']]]]]}}" value="{{form.username}}" bindinput="__e"/></view><view class="input-item"><text class="tit">密码：</text><input name="password" type="password" placeholder="请输入您的密码:" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['form']]]]]}}" value="{{form.password}}" bindinput="__e"/></view><view class="input-item"><text class="tit">确认密码：</text><input name="password2" type="password" placeholder="请输入确认密码:" data-event-opts="{{[['input',[['__set_model',['$0','password2','$event',[]],['form']]]]]}}" value="{{form.password2}}" bindinput="__e"/></view><button class="{{['confirm-btn','bg-'+themeColor.name]}}" open-type="getUserInfo" data-event-opts="{{[['tap',[['register']]]]}}" bindtap="__e">立即注册</button></view></view></view></view>